<?xml version='1.0' encoding='utf-8'?><widget     xmlns="http://www.w3.org/ns/widgets"     xmlns:gap="http://phonegap.com/ns/1.0"    xmlns:cdv="http://cordova.apache.org/ns/1.0"    id="de.mobc.empty"     version="1.0.2"    >    <name>Empty</name>    <description>Ionic Project empty</description>    <author email="dev@mobc.de" href="http://www.mobc.de">      2016 - mobC - Peter Busch - (Peter Busch)    </author>    <content src="index.html" />    <icon src="icon.png" />	<access origin="http://*/*" />
	<access origin="https://*/*" />
	<access origin="cdvfile://*" />
	<allow-navigation href="http://*/*" />
	<allow-navigation href="https://*/*" />
	<allow-intent href="http://*/*" />
	<allow-intent href="https://*/*" />
	<feature name="http://api.phonegap.com/1.0/device" />
	<preference name="orientation" value="default" />
	<preference name="fullscreen" value="false" />
	<preference name="SplashScreen" value="splash" />
	<preference name="AutoHideSplashScreen" value="true" />
	<preference name="SplashScreenDelay" value="3000" />
	<plugin name="cordova-plugin-device" spec="^1.1.0" source="npm" />
	<plugin name="cordova-plugin-console" spec="^1.0.2" source="npm" />
	<plugin name="cordova-plugin-whitelist" spec="^1.2.0" source="npm" />
	<plugin name="cordova-plugin-splashscreen" spec="^3.0.0" source="npm" />
	<plugin name="ionic-plugin-keyboard" spec="^1.0.8" source="npm" />
	<plugin name="cordova-plugin-file" spec="^3.0.0" source="npm" />
	<plugin name="cordova-plugin-file-transfer" spec="^1.4.0" source="npm" />
    <platform name="android">		<allow-intent href="market:*" />
		<preference name="SplashMaintainAspectRatio" value="true" />
		<preference name="AndroidPersistentFileLocation" value="Internal" />
		<preference name="xwalkVersion" value="15+" />
		<preference name="xwalkCommandLine" value="--disable-pull-to-refresh-effect" />
		<preference name="xwalkMode" value="embedded" />
		<preference name="xwalkMultipleApk" value="true" />
		<plugin name="org.crosswalk.engine" spec="1.3.0" source="pgb" />
		<icon qualifier="ldpi" src="res/android/icon/drawable-ldpi-icon.png" />
		<icon qualifier="mdpi" src="res/android/icon/drawable-mdpi-icon.png" />
		<icon qualifier="hdpi" src="res/android/icon/drawable-hdpi-icon.png" />
		<icon qualifier="xhdpi" src="res/android/icon/drawable-xhdpi-icon.png" />
		<icon qualifier="xxhdpi" src="res/android/icon/drawable-xxhdpi-icon.png" />
		<icon qualifier="xxxhdpi" src="res/android/icon/drawable-xxxhdpi-icon.png" />
		<splash qualifier="land-ldpi" src="res/android/splash/drawable-land-ldpi-screen.png" />
		<splash qualifier="land-mdpi" src="res/android/splash/drawable-land-mdpi-screen.png" />
		<splash qualifier="land-hdpi" src="res/android/splash/drawable-land-hdpi-screen.png" />
		<splash qualifier="land-xhdpi" src="res/android/splash/drawable-land-xhdpi-screen.png" />
		<splash qualifier="land-xxhdpi" src="res/android/splash/drawable-land-xxhdpi-screen.png" />
		<splash qualifier="land-xxxhdpi" src="res/android/splash/drawable-land-xxxhdpi-screen.png" />
		<splash qualifier="port-ldpi" src="res/android/splash/drawable-port-ldpi-screen.png" />
		<splash qualifier="port-mdpi" src="res/android/splash/drawable-port-mdpi-screen.png" />
		<splash qualifier="port-hdpi" src="res/android/splash/drawable-port-hdpi-screen.png" />
		<splash qualifier="port-xhdpi" src="res/android/splash/drawable-port-xhdpi-screen.png" />
		<splash qualifier="port-xxhdpi" src="res/android/splash/drawable-port-xxhdpi-screen.png" />
		<splash qualifier="port-xxxhdpi" src="res/android/splash/drawable-port-xxxhdpi-screen.png" />
    </platform> </widget>